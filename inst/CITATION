  author       = personList(as.person("Marvin Reich"),
                   as.person("Michal Mikolay"),
                   as.person("Theresa Blume"),
                   as.person("Andreas GÃ¼ntner")),
  journal      = "Geophysical Journal International",
  year         = "2017",
